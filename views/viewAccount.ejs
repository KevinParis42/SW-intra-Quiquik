<!doctype html>
<html lang="fr">
<head>
	<%- include('partials/header') %>
	<title>User list</title>
	<link href="../public/css/userPage.css" rel="stylesheet">
</head>
<body>
	<%- include('partials/navbar') %>
	<main role="main" style="padding-top: 3.5em;">
		<div class="container">
			<div class="container">
				<% if (account) {%>
					<p><%=account.wizardName %></p>
					<p><%=account.wizardLvl%></p>
					<ul>
						<% account.units.forEach(function(unit) { %>
							<li>
								<p><strong><%= unit.name %></strong> <%= unit.lvl %></p>
							</li>
						<% }); %>
					</ul>
				<% } else {%>
					<p>Cet utilisateur n'a pas encore uploader ses informations</p>
				<% }%>
			</div>
		</div>
		<!-- /container -->
		</main>

		<footer class="container">
			<p>&copy; KÃ©vin PARIS - 2020</p>
		</footer>
